app = "sc2fi-irc-discord-bridge"
kill_signal = "SIGINT"
kill_timeout = 5
processes = []

[env]
  CACHE_LIMIT = "512MB"
  DISCORD_TOKEN = ""
  YOUTUBE_API_KEY = ""
  SPOTIFY_CLIENT_ID = ""
  SPOTIFY_CLIENT_SECRET = ""

[experimental]
  allowed_public_ports = []
  auto_rollback = true

[[services]]
  http_checks = []
  internal_port = 8080
  processes = ["app"]
  protocol = "tcp"
  script_checks = []
